package aula2;

public class Metodo {

	//Uma função, chamada de métodos em Java, é sempre definida dentro de uma classe e tem como cabeçalho:
//	
//	<modificadores><tipo-de-retorno><nome-da-funcao>(paramentors recebidos){ <implementação>}
	

	
//	EXEMPLOS
	public void imprimir(String texto) {
		System.out.println("imprimiendo \n" + texto);
	}
	
	public int somar(int n1, int n2) {
		int soma;
		soma = n1 + n2;
		return soma;
	}
	
	public int soma(int n1, int n2) {
		return (n1 + n2);
	}
	
}
