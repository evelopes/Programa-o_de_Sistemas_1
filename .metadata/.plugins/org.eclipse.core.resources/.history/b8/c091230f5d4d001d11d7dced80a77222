package w3schools;

public class StringsMetodos {
	public static void main(String[] args) {
		String nome = "Jane "; //foi adicionado um espaço no final para funcionar melhor o concat
		String sobrenome = "Austen";
		System.out.println(nome + sobrenome); 
		
		//mesmo efeito com concat()
		System.out.println(nome.concat(sobrenome));
		
		String x = "10";
		String y = "25";
		String z =  x+y;
		System.out.println(z);
	}
	
}
